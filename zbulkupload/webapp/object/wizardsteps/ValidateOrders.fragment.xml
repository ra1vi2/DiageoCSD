<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form">
	<Table id="idProductsTable" items="{uploadData>/items}">
		<columns>
			<Column >
				<Text text="Distribution Channel"/>
			</Column>
			<Column >
				<Text text="Division"/>
			</Column>
			<Column >
				<Text text="Customer"/>
			</Column>
			<Column >
				<Text text="Order"/>
			</Column>
			<Column >
				<Text text="Date"/>
			</Column>
			<Column >
				<Text text="Plant"/>
			</Column>
			<!--<Column >
				<Text text="Index"/>
			</Column>-->
			<Column >
				<Text text="Quantity"/>
			</Column>
			<Column >
				<Text text="Material Code"/>
			</Column>
		</columns>
		<items>
			<ColumnListItem>
				<cells>
					<Text text="{uploadData>DC}"/>
					<Text text="{uploadData>DIV}"/>
					<Text text="{uploadData>CUSTOMER}"/>
					<Text text="{uploadData>ORDER}"/>
					<Text text="{uploadData>DATE}"/>
					<Text text="{uploadData>PLANT}"/>
					<!--<Text text="{uploadData>INDEX}"/>-->
					<Text text="{uploadData>QTY}"/>
					<Text text="{uploadData>MATERIAL}"/>
				</cells>
			</ColumnListItem>
		</items>
	</Table>
</core:FragmentDefinition>