<mvc:View 	height="100%" controllerName="com.diageo.csd.indentuploadzindentupload.controller.View1" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<NavContainer id="wizardNavContainer">
		<pages>
			<Page id="wizardContentPage" showHeader="false">
				<content>
					<Wizard id="UploadWizard" complete="wizardCompletedHandler">
						<WizardStep id="UploadFileStep" title="Upload" validated="true"></WizardStep>
						<WizardStep id="ValidateDataStep" validated="false" title="Validate" activate="additionalInfoValidation"></WizardStep>
						<WizardStep id="OptionalInfoStep" validated="true" activate="optionalStepActivation" title="Submit"></WizardStep>
					</Wizard>
				</content>
				<footer>
					<OverflowToolbar>
						<ToolbarSpacer/>
						<Button text="Cancel" press="handleWizardCancel"/>
					</OverflowToolbar>
				</footer>
			</Page>
		</pages>
	</NavContainer>
</mvc:View>