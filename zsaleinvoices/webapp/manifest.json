{
	"_version": "1.7.0",
	"sap.app": {
		"id": "com.diageo.csd.saleinvoiceszsaleinvoices",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZDLT_INDBLK_INVOICE_SRV": {
				"uri": "/sap/opu/odata/sap/ZDLT_INDBLK_INVOICE_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		},
		"crossNavigation": {
			"inbounds": {
				"manageSaleInvoice": {
					"semanticObject": "zsaleinvoice",
					"action": "manage",
					"deviceTypes": {
						"desktop": true,
						"tablet": true,
						"phone": false
					}
				}
			},
			"outbounds": {
				"requestInvoice": {
					"semanticObject": "ZDLTINVREQ_O",
					"action": "display",
					"additionalParameters": "ignored"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"fullWidth": true,
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "com.diageo.csd.saleinvoiceszsaleinvoices.app.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.collaboration": {},
				"sap.m": {},
				"sap.ui.comp": {},
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.diageo.csd.saleinvoiceszsaleinvoices.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Inline"
				},
				"dataSource": "ZDLT_INDBLK_INVOICE_SRV",
				"preload": true
			},
			"constants": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "model/constants.json",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.diageo.csd.saleinvoiceszsaleinvoices",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "listView",
					"pattern": "",
					"target": [
						"listView"
					]
				},
				{
					"name": "GenerateDocket",
					"pattern": "generateSaleDocket",
					"target": [
						"GenerateDocket"
					]
				}
			],
			"targets": {
				"GenerateDocket": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "invoicedetail.InvoiceDetail",
					"viewName": "invoicedetail.InvoiceDetail",
					"title": "Generate Docket"
				},
				"listView": {
					"viewType": "XML",
					"viewName": "view.ListView",
					"viewId": "ListView",
					"viewLevel": 1,
					"title": "Sale Invoices"
				}
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zsaleinvoices/webapp",
		"_version": "1.1.0"
	}
}